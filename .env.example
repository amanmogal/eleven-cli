# ElevenLabs API Configuration
# Get your API key from: https://elevenlabs.io/app/settings/api-keys
ELEVEN_API_KEY=your_elevenlabs_api_key_here

# Default Voice ID (ElevenLabs voice identifier)
# Common voices: EXAVITQu4vr4xnSDxMaL (Bella), ErXwobaYiN019PkySvjV (Antoni)
DEFAULT_VOICE_ID=EXAVITQu4vr4xnSDxMaL

# Cursor CLI Configuration
# Path to cursor-agent executable (usually 'cursor-agent' if installed globally)
CURSOR_AGENT_PATH=cursor-agent

# Project Configuration
CURSOR_PROJECT_PATH=.

# Application Configuration
DEBUG=false
LOG_LEVEL=info
NODE_ENV=development

# API Configuration
ELEVEN_API_BASE_URL=https://api.elevenlabs.io/v1
REQUEST_TIMEOUT=30000
MAX_RETRIES=3

# File Configuration
DEFAULT_OUTPUT_DIR=./output
TEMP_DIR=./.temp