{
  "name": "eleven-voice-cli",
  "version": "1.0.4",
  "description": "CLI for building ElevenLabs voice agents with AI assistance",
  "main": "bin/index.js",
  "bin": {
    "eleven": "./bin/index.js"
  },
  "scripts": {
    "dev": "node bin/index.js",
    "start": "node bin/index.js",
    "test": "npx jest --verbose",
    "test:watch": "npx jest --watch",
    "test:coverage": "npx jest --coverage",
    "lint": "npx eslint src/ bin/ tests/ --ext .js",
    "lint:fix": "npx eslint src/ bin/ tests/ --ext .js --fix",
    "format": "npx prettier --write \"src/**/*.js\" \"bin/**/*.js\" \"tests/**/*.js\"",
    "format:check": "npx prettier --check \"src/**/*.js\" \"bin/**/*.js\" \"tests/**/*.js\"",
    "validate": "node bin/index.js --version && echo 'CLI validation passed'",
    "prepublishOnly": "npm run validate",
    "postinstall": "node -e \"console.log('\\nðŸŽ§ eleven-cli installed successfully!\\nRun \\'eleven --help\\' to get started.\\n')\""
  },
  "keywords": [
    "cli",
    "elevenlabs",
    "voice",
    "ai",
    "cursor",
    "tts",
    "voice-agents",
    "text-to-speech",
    "voice-synthesis",
    "developer-tools"
  ],
  "author": {
    "name": "Aman",
    "email": "amanmogal123@gmail.com",
    "url": "https://github.com/amanmogal"
  },
  "license": "MIT",
  "homepage": "https://github.com/silver-team/eleven-cli#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/silver-team/eleven-cli.git"
  },
  "bugs": {
    "url": "https://github.com/silver-team/eleven-cli/issues"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "boxen": "^7.0.0",
    "chalk": "^5.3.0",
    "chokidar": "^3.5.3",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.2.0",
    "highlight.js": "^11.9.0",
    "https-proxy-agent": "^7.0.2",
    "inquirer": "^9.2.12",
    "marked": "^9.1.6",
    "ora": "^7.0.1",
    "semver": "^7.5.4",
    "socks-proxy-agent": "^8.0.2",
    "validate-npm-package-name": "^5.0.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js",
      "bin/**/*.js",
      "!**/node_modules/**",
      "!**/coverage/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "testMatch": [
      "**/tests/**/*.test.js",
      "**/tests/**/*.spec.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(chalk|ansi-styles|strip-ansi|has-ansi|ansi-regex|supports-color|color-convert|color-name)/)"
    ],
    "moduleNameMapper": {
      "^#ansi-styles$": "<rootDir>/node_modules/ansi-styles/index.js"
    }
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es2022": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2022,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_"
        }
      ],
      "prefer-const": "error",
      "no-var": "error"
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false
  },
  "files": [
    "bin/",
    "src/",
    "README.md",
    "LICENSE"
  ],
  "preferGlobal": true,
  "os": [
    "darwin",
    "linux",
    "win32"
  ]
}
